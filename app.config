<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup>
        <supportedRuntime    sku=".NETFramework,Version=v4.5" version="v4.0"/>
    </startup>

    <system.data>
        <DbProviderFactories>
            <add name="SqlClient Data Provider"
                 invariant="System.Data.SqlClient"
                 description=".Net Framework Data Provider for SqlServer"
                 type="System.Data.SqlClient.SqlClientFactory, System.Data,   
                    Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089"
            />
            <add name="My XML Data Provider"
                 invariant="NSMyProvider"
                 description="an XML data provider"
                 type="NSXmlDatasource.XmlProviderFactory, XmlDataSource, Version=1.0.0.0, Culture=neutral"
            />
        </DbProviderFactories>
    </system.data>
</configuration>
